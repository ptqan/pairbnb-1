<%= form_for @listing do |f| %>
	  <%= f.label :name %>
	  <%= f.text_field :name %>
	  <br/>
	  <%= f.label :description %>
	  <%= f.text_field :description %>
	  <br/>
	  <%= f.label :tags %>
	  <%= f.text_field :tags %>
