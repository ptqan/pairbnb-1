<div class="container">
	<div class="row">
		<h1>Edit your profile</h1>
		 <%= form_for @user do |f| %>

			<%= render :partial => "form", :locals=>{:f=>f} %>
			<%= link_to "Delete this image", remove_image_path(@user), method: :delete %>
		 <div class="submit-field">
      <%= f.submit %>
    </div>
  <% end %>
	</div>
</div>

